---
import { getEntry, render } from "astro:content";
import Document from "../layout/Document.astro";

const indexPage = await getEntry("docs", "index");
if (!indexPage) {
  throw new Error("Could not find index.md in content collection");
}

const { Content, headings } = await render(indexPage);
---

<Document {...indexPage.data} headings={headings}>
  <Content />
</Document>
